{
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "node": true
    },

    "globals": {
        "React": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
       "no-set-state": "off"
    }
}
